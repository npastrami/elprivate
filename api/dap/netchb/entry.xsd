				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="remarks" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="lot-value-information" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg25" type="entry:pg25Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="packaging" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See the FDA Supplemental Guide for a list of acceptable Base units of measure
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg26" type="entry:pg26Type" minOccurs="0" maxOccurs="6"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="containers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg27" type="entry:pg27Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="pg28" type="entry:pg28Type" minOccurs="0"/>
			<xsd:element name="pg29" type="entry:pg29Type" minOccurs="0"/>
			<xsd:element name="pg30" type="entry:pg30Type" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="fsisType">
		<xsd:annotation>
			<xsd:documentation>
				This is the tag that should be used for sending FSIS information
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="pg01" type="entry:pg01Type"/>
			<xsd:element name="pg06" type="entry:pg06Type" minOccurs="1" maxOccurs="2"/>
			<xsd:element name="inspection-date" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			
			<xsd:element name="location-code" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						8 - Importer Establishment Number
						10 - Egg Processing Establishment Number
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="8"/>
						<xsd:enumeration value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="location" minOccurs="1" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="form9540-signature-date" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			
			<xsd:element name="inspection-certificate" minOccurs="1" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg13" type="entry:pg13Type" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="pg14" type="entry:pg14Type" minOccurs="1" maxOccurs="1"/>
						
						<xsd:element name="lot-number" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="certificate-line-number">
										<xsd:simpleType>
											<xsd:restriction base="xsd:int">
												<xsd:maxInclusive value="999"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="net-weight">
										<xsd:annotation>
											<xsd:documentation>Net Weight in LB</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:decimal">
												<xsd:fractionDigits value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="outer-container">
										<xsd:complexType>
											<xsd:all>
												<xsd:element name="quantity">
													<xsd:simpleType>
														<xsd:restriction base="xsd:decimal">
															<xsd:fractionDigits value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="unit">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="5"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="shipping-mark">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="25"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:all>
										</xsd:complexType>
									</xsd:element>
									
									<xsd:element name="inner-container" minOccurs="0">
										<xsd:complexType>
											<xsd:all>
												<xsd:element name="quantity">
													<xsd:simpleType>
														<xsd:restriction base="xsd:decimal">
															<xsd:fractionDigits value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="unit">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="5"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="shipping-mark">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="25"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:all>
										</xsd:complexType>
									</xsd:element>
									
									<xsd:element name="category-code">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="5"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="commodity-type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="4"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="commodity-characteristic">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="4"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="producing-establishment">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="15"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="exporting-establishment">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="15"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="source-establishment" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="15"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="production-start-date" type="xsd:date" minOccurs="0"/>
									<xsd:element name="production-end-date" type="xsd:date" minOccurs="0"/>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="entities" minOccurs="1" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="entity" minOccurs="0" maxOccurs="4">
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="role-code">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:enumeration value="CI"/>
												<xsd:enumeration value="CB"/>
												<xsd:enumeration value="IM"/>
												<xsd:enumeration value="CN"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="name" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="90"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="phone-number" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="15"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="email-or-fax" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="35"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="seal-numbers" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="dotItemType">
		<xsd:annotation>
			<xsd:documentation>
				This is the base tag to include DOT information in an entry
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="box-number">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="01"/>
						<xsd:enumeration value="2A"/>
						<xsd:enumeration value="2B"/>
						<xsd:enumeration value="03"/>
						<xsd:enumeration value="04"/>
						<xsd:enumeration value="05"/>
						<xsd:enumeration value="06"/>
						<xsd:enumeration value="07"/>
						<xsd:enumeration value="08"/>
						<xsd:enumeration value="09"/>
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="11"/>
						<xsd:enumeration value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="box-certification">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="Y"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="passport-number" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Required for box number 5
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="19"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="country" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						country is only necessary for box 05, 06, or 12
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="data:countryCodeType"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dot-bond" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="1000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="nhtsa-permission" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						nhtsa-permission is required for box 2B, 06, 07, 08, or 09
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="importers-substantiating-statement" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						required for box 2B, 03, 07, 08, 09
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="clarification-code" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						V = Vehicle, E = Equipment, T = Tire
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="V"/>
						<xsd:enumeration value="E"/>
						<xsd:enumeration value="T"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="tire-manufacturer-id" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Should only be included if clarification code is T</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="tire-manufacturer-brand-name" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Should only be included if clarification code is T</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="nhtsa-registered-importer-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="8"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="vehicle-eligibility-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="6"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="make" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="model" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="year" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>must be the 4 digit year</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1850" />
						<xsd:maxInclusive value="2100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="vin" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="vins" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Though not yet operational, DOT states that in the future,
						it should be possible to report multiple VINS in a single DOT
						line item.  So this tag is for future use only.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="vin" minOccurs="1" maxOccurs="unbounded">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="17"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="nhtsaType">
		<xsd:annotation>
			<xsd:documentation>
				Tag for including NHTSA data
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="pg01" type="entry:pg01Type" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="pg07" type="entry:pg07Type" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="pg08" type="entry:pg08Type" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="pg10" type="entry:pg10Type" minOccurs="1" maxOccurs="unbounded"/>
			<xsd:element name="pg14" type="entry:pg14Type" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="pg22" type="entry:pg22Type" minOccurs="1" maxOccurs="unbounded"/>
			<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="pg34" type="entry:pg34Type" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="pg35" type="entry:pg35Type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="nmfsType">
		<xsd:annotation>
			<xsd:documentation>
				Tag for including NMFS and SIMP data
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="pg14" type="entry:pg14Type" minOccurs="0" maxOccurs="unbounded"/>
			
			<xsd:choice>
				<xsd:element name="disclaim370" minOccurs="0" maxOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="A"/>
							<xsd:enumeration value="B"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="disclaimAmr" minOccurs="0" maxOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="A"/>
							<xsd:enumeration value="B"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="disclaimHms" minOccurs="0" maxOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="A"/>
							<xsd:enumeration value="B"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="nmfs-program" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="pg01" type="entry:pg01Type" minOccurs="1" maxOccurs="1"/>
							
							<xsd:element name="origin-information" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="pg06" type="entry:pg06Type" minOccurs="1" maxOccurs="1"/>
										<xsd:element name="pg22" type="entry:pg22Type" minOccurs="1" maxOccurs="unbounded"/>
										<xsd:element name="harvesting-vessel-country" minOccurs="0" maxOccurs="unbounded" type="data:countryCodeType"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							
							<xsd:element name="pg22" type="entry:pg22Type" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
			
			<xsd:element name="sim-program" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="electronic-image-submitted" minOccurs="0"/>
						<xsd:element name="confidential-information" minOccurs="1"/>
						<xsd:element name="pg05" type="entry:pg05Type" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="pg06" type="entry:pg06Type" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
						<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="unbounded"/>
						<xsd:element name="pg29" type="entry:pg29Type" minOccurs="0" maxOccurs="unbounded"/>
						
						<xsd:element name="svh-country-of-first-landing" minOccurs="0" maxOccurs="1" type="data:countryCodeType"/>
						<xsd:element name="svh-harvesting-vessel-name" minOccurs="0" maxOccurs="unbounded">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="68"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						
						<xsd:element name="vessel-info" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg31" minOccurs="0" maxOccurs="7">
										<xsd:complexType>
											<xsd:all>
												<xsd:element name="vessel-characteristic-type-code">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:enumeration value="VCR" />
															<xsd:enumeration value="VNM" />
															<xsd:enumeration value="IMO" />
															<xsd:enumeration value="VCS" />
															<xsd:enumeration value="PMT" />
															<xsd:enumeration value="RFO" />
															<xsd:enumeration value="OTH" />
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="vessel-characteristic">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="35"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="harvested-weight" minOccurs="0">
													<xsd:simpleType>
														<xsd:restriction base="xsd:decimal">
															<xsd:minInclusive value="0" />
															<xsd:maxExclusive value="100000000"/>
															<xsd:fractionDigits value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												
												<xsd:element name="weight-unit-of-measure" minOccurs="0">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="3"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:all>
										</xsd:complexType>
									</xsd:element>
									
									<xsd:element name="country-of-transshipment" minOccurs="0" type="data:countryCodeType"/>
									<xsd:element name="country-of-first-landing" minOccurs="0" type="data:countryCodeType"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="ttbType">
		<xsd:annotation>
			<xsd:documentation>
				Tag for including TTB PGA information
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="processing-code">
				<xsd:annotation>
					<xsd:documentation>
						See ACE CATAIR, TTB chapter for descriptions
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="T01"/>
						<xsd:enumeration value="T02"/>
						<xsd:enumeration value="T03"/>
						<xsd:enumeration value="T04"/>
						<xsd:enumeration value="T05"/>
						<xsd:enumeration value="T06"/>
						<xsd:enumeration value="T07"/>
						<xsd:enumeration value="T08"/>
						<xsd:enumeration value="T09"/>
						<xsd:enumeration value="T10"/>
						<xsd:enumeration value="T11"/>
						<xsd:enumeration value="T12"/>
						<xsd:enumeration value="T13"/>
						<xsd:enumeration value="T14"/>
						<xsd:enumeration value="T15"/>
						<xsd:enumeration value="T16"/>
						<xsd:enumeration value="T17"/>
						<xsd:enumeration value="T21"/>
						<xsd:enumeration value="T22"/>
						<xsd:enumeration value="T30"/>
						<xsd:enumeration value="T34"/>
						<xsd:enumeration value="T35"/>
						<xsd:enumeration value="T36"/>
						<xsd:enumeration value="T37"/>
						<xsd:enumeration value="T39"/>
						<xsd:enumeration value="T40"/>
						<xsd:enumeration value="T41"/>
						<xsd:enumeration value="T42"/>
						<xsd:enumeration value="T43"/>
						<xsd:enumeration value="T44"/>
						<xsd:enumeration value="T45"/>
						<xsd:enumeration value="T46"/>
						<xsd:enumeration value="T51"/>
						<xsd:enumeration value="T52"/>
						<xsd:enumeration value="T54"/>
						<xsd:enumeration value="T55"/>
						<xsd:enumeration value="T56"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="permits" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="permit" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="permit-type">
										<xsd:annotation>
											<xsd:documentation>
												Permit Types:
												1 - Permit Number
												2 - Certificate of Label Approval (COLA) Number
												3 - Foreign Certificate
												4 - IRC Reg. Number for Release Without Payment of Tax for Beer, Wine or Spirits
												5 - Permit Number for Release Without Tax for Tobacco Products
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:enumeration value="1"/>
												<xsd:enumeration value="2"/>
												<xsd:enumeration value="3"/>
												<xsd:enumeration value="4"/>
												<xsd:enumeration value="5"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="permit-number" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="33"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="permit-number-exemption-code" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												See ACE CATAIR, TTB chapter for descriptions
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:enumeration value="TTBEX1"/>
												<xsd:enumeration value="TTBEX2"/>
												<xsd:enumeration value="TTBEX3"/>
												<xsd:enumeration value="TTBEX4"/>
												<xsd:enumeration value="TTBEX5"/>
												<xsd:enumeration value="TTBEX6"/>
												<xsd:enumeration value="TTBEX7"/>
												<xsd:enumeration value="TTBEX8"/>
												<xsd:enumeration value="TTBEX9"/>
												<xsd:enumeration value="TTBEX10"/>
												<xsd:enumeration value="TTBEX11"/>
												<xsd:enumeration value="TTBEX12"/>
												<xsd:enumeration value="TTBEX13"/>
												<xsd:enumeration value="TTBEX14"/>
												<xsd:enumeration value="TTBEX15"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="importers-substantiating-statement" minOccurs="0" type="xsd:boolean"/>
									
									<xsd:element name="certificate-issuing-country" minOccurs="0" type="data:countryCodeType">
										<xsd:annotation>
											<xsd:documentation>
												***Only include this tag if the permit typs is 3***
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="cigarette-paper-tube-info" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This tag should only be included if the processing code is T51-T56 
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="quantity">
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:minExclusive value="0"/>
									<xsd:fractionDigits value="2"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						
						<xsd:element name="units">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="cigar-quantities" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This tag should only be included if the processing code is T41-T43
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="cigar-quantity" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="quantity">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minExclusive value="0"/>
												<xsd:maxExclusive value="1000000"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									
									<xsd:element name="unit-price">
										<xsd:complexType>
											<xsd:choice>
												<xsd:element name="price" type="xsd:double"/>
												<xsd:element name="maximum-rate"/>
												<xsd:element name="small-cigars"/>
											</xsd:choice>
										</xsd:complexType>
									</xsd:element>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="fccItemType">
		<xsd:annotation>
			<xsd:documentation>
				This is the base tag for including FCC information with an entry
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="import-condition-number">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="01"/>
						<xsd:enumeration value="02"/>
						<xsd:enumeration value="03"/>
						<xsd:enumeration value="04"/>
						<xsd:enumeration value="05"/>
						<xsd:enumeration value="5A"/>
						<xsd:enumeration value="06"/>
						<xsd:enumeration value="07"/>
						<xsd:enumeration value="08"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="import-condition-03-quantity-approval" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This should only be included if the import condition number is
						03 and the quantity imported is greater than 200 units
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fcc-identifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This is required for import considition number 01
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="trade-name">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="model-number">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="quantity">
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0"/>
						<xsd:maxExclusive value="1000000000000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="withhold-from-public-inspection-requested" minOccurs="0" />
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="fwsPgaType">
		<xsd:annotation>
			<xsd:documentation>
				This is the tag that should be used for sending FWS information for ACE Cargo Release
				The following optional tags are required if Agency Processing Code is EDS
				1. intended-use-code
				2. species-country-origin
				3. general-common-name
				4. cartons-containing-wildlife
				5. commodity-qualifier-code
				6. line-value
				7. line-net-quantity
				8. line-net-units
				9. signature-date
				10. origin-information
				11. entities
				12. inspection-status
				13. marketing-information
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="agency-processing-code">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="EDS"/>
						<xsd:enumeration value="LDS"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="intended-use-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="16"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>	
			<xsd:element name="species-country-origin" type="data:countryCodeType" minOccurs="0"/>
			<xsd:element name="electronic-image-submitted" minOccurs="0" />
			<xsd:element name="specific-common-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="general-common-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="live-venomous-wildlife-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="Y"/>
						<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="cartons-containing-wildlife" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="commodity-qualifier-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="W"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="F"/>
						<xsd:enumeration value="P-2"/>
						<xsd:enumeration value="U-6"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="I"/>
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="X"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-value" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-net-quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-net-units" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix F for valid units
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="signature-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="origin-information" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="origin" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								 <xsd:sequence>
									<xsd:element name="pg05" type="entry:pg05Type" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="lpco-information" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="lpco" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="lpco-type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="3"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="lpco-number">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="33"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="entities" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="inspection-status" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg30" type="entry:pg30Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="marketing-information" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="remark" minOccurs="0" maxOccurs="unbounded">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="68"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="amsPgaType">
		<xsd:annotation>
			<xsd:documentation>
				Tag for including AMS PGA data
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="pg01" type="entry:pg01Type">
				<xsd:annotation>
					<xsd:documentation>
						Use confidential-information element for 'Disclaim'
						Use electronic-image-submitted element for 'Document submitted to DIS'
						The program codes for AMS PGA include role codes:
							MO1 (Request Inspection)
							MO2 (Request Admittance, Previous Inspection)
							MO3 (Request Admittance, Exemption Permit)
							MO4 (Request Exemption Permit)
							MO5 (Aflatoxin Testing of Pistachios)
							MO6 (Exempt Variety or Type)
							MO7 (Seasonal Exemption)
							MO8 (Minimum Exemption)
							EG1 (Request inspection of eggs/egg products)
							EG2 (Request inspection of eggs from Paper Form LPS-222)
							PN1 (Request Peanut Inspection)
							OR1 (Identify Certified Organic Products)
							OR2 (Certified Organic Products, Electronic Import Certificate)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="disclaimed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[
							The <disclaimed /> tag is optional. If included, NetChb will automatically disclaim the AMS PGA.
							Commercial description and program code are also required when disclaiming AMS PGA.
						]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pg02" type="entry:pg02Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Use product-code element for 'UNSPSC Number'
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pg30" type="entry:pg30Type" minOccurs="0" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>
						In one pg30 element, use arrival-time for 'Available For Inspection Date/Time'
							and arrival-location-code for 'Inspection FIRMS'
							and arrival-location-description for 'Inspection Location'
							and set inspection-status to 'I'
						In a second pg30 element, use arrival-time for 'Arrival Date'
							and set inspection-status to 'A'
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="licenses" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="license" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="pg13" type="entry:pg13Type" minOccurs="0"/>
									<xsd:element name="pg14" type="entry:pg14Type"/>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="entities" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The entities for AMS PGA include role codes:
							LG  (Locaiton of Goods)
							AP1 (Applicant Requesting Inspection)
							IM  (Importer)
							CN  (Consignee)
							EX  (Exporter)
							ORI (Certificate Issuer)
							UC  (Ultimate Consignee)
							ORC (Cerfifier of Final Handler)
							ORP (Final Handler)
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="packaging" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See the AMS Supplemental Guide for a list of acceptable units of measure
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg26" type="entry:pg26Type" minOccurs="0" maxOccurs="6"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="lot-numbers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg25" type="entry:pg25Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="net-quantity" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg29" type="entry:pg29Type" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="containers" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The container type can be:
							1 (Refrigerated)
							2 (Not Refrigerated)
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg27" type="entry:pg27Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="organic-standard" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The Organic Certification Standard code can be:
							A10 (USDA Organic Standard)
							A11 (Equivalent Organic Standard)
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="laceyPgaType">
		<xsd:annotation>
			<xsd:documentation>
				This is the tag that should be used for sending Lacey Act information.
				The entity tag is for the Entity Making Declaration field (use the role-code tag)
				Entity Contact Names, Phone Numbers, Emails can be entered within that entity's pg21 tags.
				The "Declaring Entity Certifies Data" checkbox gets auto-selected and thus will be done
				automatically, so no tag is necessary for this field.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="line-value" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="commercial-description" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="declaration-signed-date" type="xsd:date" minOccurs="1"/>
			<xsd:element name="entities" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="entity" type="entry:pgEntityType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="product-component-information" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="product-component" minOccurs="1" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									Include the active-ingredient tag in pg04 to indicate "Unknown Harvest Source"
									The source-type-code must be "HRV" in pg06 tags for "Country of Harvest""
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg04" type="entry:pg04Type" minOccurs="1" maxOccurs="1"/>
									<xsd:element name="pg05" type="entry:pg05Type" minOccurs="0" maxOccurs="1"/>
									<xsd:element name="pg06" type="entry:pg06Type" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="containers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg27" type="entry:pg27Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="lpco-information" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg14" type="entry:pg14Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="deaItemType">
		<xsd:annotation>
			<xsd:documentation>
				This is the base tag to include DEA information in an entry.
				The dea drug code is specified by the tag product-number in pg04Type
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="country-of-shipment" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="data:countryCodeType"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dea-permit-number" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dea-registration-number" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="arrival-date" type="xsd:date" minOccurs="1"/>
			<xsd:element name="dea-form" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required
						911 = DEA-35: Import permit for controlled substance in schedule I or II or narcotic controlled substance in schedule III, IV, or V
						921 = DEA-236: Declaration for the import of any non-narcotic controlled substance listed in schedule III, IV, or V
						922 = DEA-486: Declaration for import of List I or II substance except Ephedrine, Pseudoephedrine, or Phenylpropanolamine
						923 = DEA-486A: Declaration for the import of Ephedrine, Pseudoephedrine, or Phenylpropanolamine
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="911"/>
						<xsd:enumeration value="921"/>
						<xsd:enumeration value="922"/>
						<xsd:enumeration value="923"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="constituent-elements" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="constituent-element" minOccurs="1" maxOccurs="unbounded" type="entry:pg04Type">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
	<xsd:complexType name="pg01Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG01 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="agency-program-code" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="agency-processing-code" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="electronic-image-submitted" minOccurs="0"/>
			<xsd:element name="product-id-qualifier" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="product-id" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="19"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="intended-use-code" minOccurs="0" maxOccurs="unbounded">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="16"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="intended-use-description" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="22"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="confidential-information" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg02Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG02 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="item-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						P = Product
						C = Component
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="P"/>
						<xsd:enumeration value="C"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="product-code-qualifier" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="product-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="19"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg04Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG05 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="active-ingredient" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This tag is optional.  Including it means the element specified in the PG04 record is an active ingredient
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name-of-element" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="51"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="product-number" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This tag is optional. For DEA pga, this will populate the dea drug code.
						For EPA and HFC it will populate Component number and Product Code Number respectively.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="quantity-of-element" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="unit-of-measure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix PGA for acceptable values
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="percent-of-element" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Enter as percent.  For example 45% should be submitted as 45.0000 or 45, and NOT .45
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
						<xsd:fractionDigits value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg05Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG05 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="genus-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="22"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="species-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="22"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="sub-species-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="18"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="species-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="description-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg06Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG06 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="source-type-code">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix PGA for a list of acceptable codes
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="country-code" type="data:countryCodeType" minOccurs="0"/>
			<xsd:element name="geographic-location" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="processing-start-date" minOccurs="0" type="xsd:date"/>
			<xsd:element name="processing-end-date" minOccurs="0" type="xsd:date"/>
			<xsd:element name="processing-type-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="processing-description" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg07Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG07 record for PGA records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="brand-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="model" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="manufacture-month" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="manufacture-year" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1000"/>
						<xsd:maxInclusive value="2200"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="id-number-type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="id-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg08Type">
		<xsd:annotation>
			<xsd:documentation>
				PG08 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id-number" minOccurs="0" maxOccurs="unbounded">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg10Type">
		<xsd:annotation>
			<xsd:documentation>
				PG10 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="category-type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="6"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="category-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="commodity-qualifier-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="commodity-characteristic-qualifier" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="commodity-characteristic-description" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="57"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg13Type">
		<xsd:annotation>
			<xsd:documentation>
				PG13 Record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="issuer-of-lpco" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="location-code-qualifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Canadian Province = PR
						Country Code = ISO
						European Union = EU
						Mexican State = MS
						US State = US
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="PR"/>
						<xsd:enumeration value="ISO"/>
						<xsd:enumeration value="EU"/>
						<xsd:enumeration value="MS"/>
						<xsd:enumeration value="US"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="location-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="location-description" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg14Type">
		<xsd:annotation>
			<xsd:documentation>
				PG14 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="transaction-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						1 - Single Use
						2 - Continuous
						3 - General
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="license-type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="license-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="33"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="lpco-date-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						1 - Expiration Date
						2 - Effective Date
						3 - Date Issued or Signed
						4 - Date Application Received
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="lpco-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="unit" minOccurs="0">
				<!-- See Appendix C of the ACS catair for units of measure -->
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg17Type">
		<xsd:annotation>
			<xsd:documentation>
				PG17 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="specific-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="general-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pgEntityType">
		<xsd:annotation>
			<xsd:documentation>
				This includes data in the PG19, PG20 records
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="role-code">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix PGA for a list of role codes
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="id-type-code" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						16 - DUNS
						47 - FEI
						MID - MID
						331 - AMS Entity Number
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="16"/>
						<xsd:enumeration value="47"/>
						<xsd:enumeration value="MID"/>
						<xsd:enumeration value="331"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="id-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="105"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="address1" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="96"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="address2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="105"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="suite-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="city" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="94"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="state" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="country" type="data:countryCodeType" minOccurs="0"/>
			<xsd:element name="postal-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="contacts" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="pg21" type="entry:pg21Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg21Type">
		<xsd:annotation>
			<xsd:documentation>
				This is the PG21 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="90"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="phone-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-or-fax" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg22Type">
		<xsd:annotation>
			<xsd:documentation>
				PG22 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="importers-substantiating-statement" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="document-type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="conformance-declaration" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="entity-role-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="signature-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="invoice-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="17"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="compliance-description" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg23Type">
		<xsd:annotation>
			<xsd:documentation>
				The PG23 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="compliance-code">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix PGA for a list of valid compliance codes
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="compliance-qualifier" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg24Type">
		<xsd:annotation>
			<xsd:documentation>
				The PG24 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="remarks-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix PGA for valid type codes
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="remarks-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="remark" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="68"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg25Type">
		<xsd:annotation>
			<xsd:documentation>
				The PG25 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="line-value" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="unit-value" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="temperature-qualifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A - Ambient
						D - Dry Ice
						F - Frozen
						H - Fresh
						P - Flashpoint
						R - Refrigerated/Chilled
						U - Uncontrolled
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="F"/>
						<xsd:enumeration value="H"/>
						<xsd:enumeration value="P"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="U"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="temperature" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="degree-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						C - Celsius
						F - Fahrenheit
						K - Kelvin
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="F"/>
						<xsd:enumeration value="K"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="location-of-temperature-recording" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A - Product
						B - Container
						C - Conveyance
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="C"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="lot-number-qualifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						1 - Manufacturer
						2 - Seller
						3 - Grower
						4 - Producer
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="lot-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="production-start-date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="production-end-date" type="xsd:date" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg26Type">
		<xsd:annotation>
			<xsd:documentation>
				PG26 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:fractionDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="unit-of-measure" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg27Type">
		<xsd:annotation>
			<xsd:documentation>
				PG27 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="container-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="length" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg28Type">
		<xsd:annotation>
			<xsd:documentation>
				PG28 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="can-dimension-1" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:pattern value="\d{0,4}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="can-dimension-2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:pattern value="\d{0,4}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="can-dimension-3" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:pattern value="\d{0,4}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg29Type">
		<xsd:annotation>
			<xsd:documentation>
				PG29 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="line-net-quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-net-units" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix F for valid units
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-gross-quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="line-gross-units" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix F for valid units
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual-net-quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual-net-units" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix F for valid units
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual-gross-quantity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual-gross-units" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						See Appendix F for valid units
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg30Type">
		<xsd:annotation>
			<xsd:documentation>
				PG30 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="inspection-status" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="arrival-time" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:dateTime"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="arrival-location-code" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="arrival-location-description" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg32Type">
		<xsd:annotation>
			<xsd:documentation>
				PG32 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="routing-type" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="routing-country" minOccurs="0" type="data:countryCodeType"/>
			<xsd:element name="routing-subentity" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="55"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg34Type">
		<xsd:annotation>
			<xsd:documentation>
				PG34 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="travel-document-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						1 - Passport Number
						2 - Visa Number
						3 - Enhanced Tribal Card
						4 - Driver's License Number
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="travel-document-country" minOccurs="0" type="data:countryCodeType"/>
			<xsd:element name="travel-document-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="pg35Type">
		<xsd:annotation>
			<xsd:documentation>
				PG35 record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="surety-code" type="data:suretyCodeType" minOccurs="0"/>
			<xsd:element name="bond-number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="bond-type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						1 - Single
						2 - Continuous
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="bond-amount" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99999999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

	<!-- **************************************************************** -->
	<xsd:complexType name="emailNotificationsType">
		<xsd:all>
			<xsd:element name="email-address" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-cargo-release" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						If you would like this entity to receive Customs generated Cargo Release status updates via email,
						this is a boolean value must be one of the following {true, false, 1, 0}.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-manifest-updates" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						If you would like this entity to receive Customs generated Manifest updates via email,
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-oga-updates" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						If you would like this entity to receive Customs generated OGA updates via email,
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="include-entered-value" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="include-duties-fees" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-7501-on-accept" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email-7533" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}.  This should always be false if mode of transport does not begin with 2 or 3.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="attach-3461-alt-to-7533" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}.  This should always be false if mode of transport does not begin with 2 or 3.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="creator-email-cargo-release" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="creator-email-manifest-updates" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="creator-email-oga-updates" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						this is a boolean value must be one of the following {true, false, 1, 0}
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
	<!-- **************************************************************** -->
