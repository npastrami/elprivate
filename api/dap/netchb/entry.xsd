	<xsd:complexType name="fdaItemType">
		<xsd:annotation>
			<xsd:documentation>
				This is the base tag for a group of tags to represent fda information
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fda-product-code">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="country-of-production">
				<xsd:simpleType>
					<xsd:restriction base="data:countryCodeType" />
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fda-manufacturer">
				<xsd:simpleType>
					<xsd:restriction base="data:manufacturerIdType" />
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fda-supplier">
				<xsd:simpleType>
					<xsd:restriction base="data:manufacturerIdType" />
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="cargo-storage-status" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="F"/>
						<xsd:enumeration value="R"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="value" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="10000000000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fei" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:pattern value="\d{1,12}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="brand-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="38"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="container-dimension-1" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="10000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="container-dimension-2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="10000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="container-dimension-3" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="10000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="contact-name" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="contact-phone" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0" />
						<xsd:maxExclusive value="10000000000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fda-quantities" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fda-quantity" minOccurs="1" maxOccurs="6">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="quantity" minOccurs="1" maxOccurs="1">
										<xsd:simpleType>
											<xsd:restriction base="xsd:decimal">
												<xsd:minInclusive value="0"/>
												<xsd:fractionDigits value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="unit" minOccurs="1" maxOccurs="1">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="4"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="compliance-codes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="compliance-code" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="code">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="3"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="qualifier">
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:maxLength value="25"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	
