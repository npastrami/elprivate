	<xsd:complexType name="epaPgaType">
		<xsd:annotation>
			<xsd:documentation>
				This is the tag that should be used for sending EPA information for ACE Cargo Release
			</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="commercial-description">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="tsca" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="EP4"/>
						<xsd:enumeration value="EP5"/>
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="D"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="ods" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="EP4"/>
						<xsd:enumeration value="EP5"/>
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="D"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="ods-submitted-via-dis" minOccurs="0"/>
			
			<xsd:element name="certifying-individual" minOccurs="0">
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="name">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="90"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="phone">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="88"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="email">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="88"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="pesticide" minOccurs="0">
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="pg01" type="entry:pg01Type">
							<xsd:annotation>
								<xsd:documentation>
									For this PG01 record, the agency-program-code should be one of the following:
									PS1
									PS2
									PS3
									To disclaim Pesticides while sending TSCA or ODS use one of the following agency-program-codes:
									A1 = A - PS1 - Product is not regulated by this agency
									A2 = A - PS2 - Product is not regulated by this agency
									C1 = C - PS1 - Data filed through other agency means
									C2 = C - PS2 - Data filed through other agency means
									D1 = D - PS1 - Data filed through paper
									D2 = D - PS2 - Data filed through paper
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="pg02" type="entry:pg02Type"/>
						<xsd:element name="pg07" type="entry:pg07Type"/>
						<xsd:element name="pg14" type="entry:pg14Type"/>
						
						<xsd:element name="entities">
							<xsd:annotation>
								<xsd:documentation>
									The entities for pesticides should include role codes:
									CAR (carrier)
									DEQ (shipper)
									LG (location of goods)
									EPN (EPA Producer Establishment No)
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="pg22" type="entry:pg22Type"/>
						
						<xsd:element name="remarks" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="packaging">
							<xsd:annotation>
								<xsd:documentation>
									See the FDA Supplemental Guide for a list of acceptable Base units of measure
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg26" type="entry:pg26Type" minOccurs="2" maxOccurs="6"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="pg29" type="entry:pg29Type"/>
						
						<xsd:element name="components" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg04" type="entry:pg04Type" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="vehicle-engine" minOccurs="0">
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="pg01" type="entry:pg01Type">
							<xsd:annotation>
								<xsd:documentation>
									For this PG01 record, the agency-program-code should be one of the following:
									ORD
									OFF
									
									ORD = On Road Vehicle
									OFF = Off Road Vehicle 
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="pg02" type="entry:pg02Type"/>
						
						<xsd:element name="import-codes">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg24" type="entry:pg24Type" minOccurs="1" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="vehicles-or-engines">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="vehicle-or-engine" minOccurs="1" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="pg07" type="entry:pg07Type" minOccurs="1" maxOccurs="1"/>
												<xsd:element name="pg08" type="entry:pg08Type" minOccurs="0" maxOccurs="1"/>
												
												<xsd:element name="components" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="pg07" type="entry:pg07Type" minOccurs="0" maxOccurs="unbounded"/>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="characteristics" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg10" type="entry:pg10Type" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="licenses" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg14" type="entry:pg14Type" minOccurs="1" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="entities">
							<xsd:annotation>
								<xsd:documentation>
									The entities for vehicles should include role codes:
									DFP (owner)
									MF (manufacturer)
									CI (certifying individual)
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="entity" type="entry:pgEntityType" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

						<xsd:element name="pg22" type="entry:pg22Type"/>
						
						<xsd:element name="remarks" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="pg24" type="entry:pg24Type" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						
						<xsd:element name="additional-components" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="component" minOccurs="0" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="pg07" type="entry:pg07Type" minOccurs="1" maxOccurs="1"/>
												
												<xsd:element name="manufacturer-name" minOccurs="1" maxOccurs="1">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="105"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="license-type" minOccurs="1" maxOccurs="1">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="3"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="license-number" minOccurs="1" maxOccurs="1">
													<xsd:simpleType>
														<xsd:restriction base="xsd:token">
															<xsd:maxLength value="33"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>

