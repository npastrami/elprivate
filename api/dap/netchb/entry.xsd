	<!-- **************************************************************** -->
	<xsd:complexType name="disclaimPgaType">
		<xsd:all>
			<xsd:element name="default-disclaim-reason" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A - Product is not regulated by this agency
						B - Data is not required per agency guidance
						
						Tobacco
						Q - Product is not regulated by this agency
						R - Data is not required per agency guidance
						S - Data filed through other agency means
						T - Data filed through paper
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="Q"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="T"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element name="epa-disclaim" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="epa-disclaim-reason" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									A - Product is not regulated by this agency
									C - Data filed through other agency means
									D - Data filed through paper
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:enumeration value="A"/>
									<xsd:enumeration value="C"/>
									<xsd:enumeration value="D"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						
						<xsd:element name="epa-program-code" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									PS1 - REGISTERED PESTICIDES
									PS2 - PESTICIDES - DEVICES
									PS3 - PESTICIDES - OTHER
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:enumeration value="PS1"/>
									<xsd:enumeration value="PS2"/>
									<xsd:enumeration value="PS3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="aphis-disclaim" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="aphis-disclaim-reason" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									A - Product is not regulated by this agency
									B - Data is not required per agency guidance
									G - Plant material is less than or equal to 5% of unit weight and doesn't exceed 2.9kg
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:enumeration value="A"/>
									<xsd:enumeration value="B"/>
									<xsd:enumeration value="G"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						
						<xsd:element name="aphis-program-code" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									AAC - ANIMAL CARE
									ABS - BIOTECH REGULATORY SERVICES
									APQ - PLANT PROTECTION / QUARANTINE
									AVS - VETERINARY SERVICES
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:enumeration value="AAC"/>
									<xsd:enumeration value="ABS"/>
									<xsd:enumeration value="APQ"/>
									<xsd:enumeration value="AVS"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="fws-disclaim-reason" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						C - Form 3-177 and accompanying documents will be filed in eDecs after ACE filing
						D - Form 3-177 and accompanying documents were cleared via paper directly with FWS
						E - Product does not contain fish or wildlife, including live, dead, parts or products thereof
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="E"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
</xsd:schema>